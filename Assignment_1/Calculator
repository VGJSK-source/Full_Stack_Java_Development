public class Calc1 {
   public static void main(String[] args) {
        int a = Integer.parseInt(args[0]);
        int b = Integer.parseInt(args[2]);
        char c = args[1].charAt(0);
        
        //if( args[1].equals("+") ) System.out.println(a+b);

        if(c=='+')
        System.out.println(a+b);
        else if(c=='-')
        System.out.println(a-b);
        else if(c=='x')
        System.out.println(a*b);
        else if(c=='/')
        System.out.println(a/b);
   }
}
